{
  "name": "node-spring-config-client",
  "version": "1.0.0",
  "description": "Node module for node services to get configuration from spring config server and support refreshing configs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc && yarn lint && yarn test",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "preversion": "yarn lint"
  },
  "keywords": [
    "node",
    "spring",
    "config",
    "client"
  ],
  "author": "Parth Patel",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/request": "^2.47.1",
    "es6-symbol": "^3.1.1",
    "request": "^2.87.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "jest": "^23.4.1",
    "prettier": "^1.14.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.14.0"
  }
}
